  <simpleType name="LystypeType">
    <annotation>
      <documentation>hinderlys til bruk ved merking av luftfartshinder i henhold til vedlegg til forskrift om rapportering, registrering og merking av luftfartshinder, BSL E 2-1. &lt;font color="#0f0f0f"&gt;I tillegg er det mulig å velge koder fra gammel NRL&lt;/font&gt;</documentation>
    </annotation>
    <union memberTypes="app:LystypeEnumerationType app:LystypeOtherType"/>
  </simpleType>
  <simpleType name="LystypeEnumerationType">
    <annotation>
      <documentation>hinderlys til bruk ved merking av luftfartshinder i henhold til vedlegg til forskrift om rapportering, registrering og merking av luftfartshinder, BSL E 2-1. &lt;font color="#0f0f0f"&gt;I tillegg er det mulig å velge koder fra gammel NRL&lt;/font&gt;</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="LIA">
        <annotation>
          <documentation>rødt, fast, 10 candela</documentation>
        </annotation>
      </enumeration>
      <enumeration value="LIB">
        <annotation>
          <documentation>rødt, fast, 32 candela</documentation>
        </annotation>
      </enumeration>
      <enumeration value="LIC">
        <annotation>
          <documentation>ikke del av oversikten i vedlegg 2 BSL E 2-1, men tatt fra tabell 6-3 i ICAO annex 14</documentation>
        </annotation>
      </enumeration>
      <enumeration value="LID">
        <annotation>
          <documentation>ikke del av oversikten i vedlegg 2 BSL E 2-1, men tatt fra tabell 6-3 i ICAO annex 14</documentation>
        </annotation>
      </enumeration>
      <enumeration value="MIA">
        <annotation>
          <documentation>hvitt, blinkende, 20 000 candela ved bakgrunnsbelysning 50 cd/m ² og over, 2000 candela med bakgrunnsbelysning uneder 50 cd/m ²</documentation>
        </annotation>
      </enumeration>
      <enumeration value="MIB">
        <annotation>
          <documentation>rødt, blinkende, 2000 candela</documentation>
        </annotation>
      </enumeration>
      <enumeration value="MIC">
        <annotation>
          <documentation>rødt, fast, 2000 candela</documentation>
        </annotation>
      </enumeration>
      <enumeration value="HIA">
        <annotation>
          <documentation>hvitt, blinkende, 200 000 candela ved bakgrunnsbelysning over 500 cd/m², 20 000 candela ved bakgrunnsbelysning 50-500 cd/m² og 2000 candela ved bakgrunnsbelysning under 50 cd/m²</documentation>
        </annotation>
      </enumeration>
      <enumeration value="HIB">
        <annotation>
          <documentation>hvitt, blinkende, 100 000 candela ved bakgrunnsbelysning over 500 cd/m², 20 000 candela ved bakgrunnsbelysning 50-500 cd/m² og 2000 candela ved bakgrunnsbelysning under 50 cd/m²</documentation>
        </annotation>
      </enumeration>
      <enumeration value="FLO">
        <annotation>
          <documentation>objektet er opplys av lyskastere for å være synlig for luftfarten</documentation>
        </annotation>
      </enumeration>
      <enumeration value="FR">
        <annotation>
          <documentation>brukes dersom vi ikke vet lysstyrke, men har observert lysfarge rød fast lys</documentation>
        </annotation>
      </enumeration>
      <enumeration value="FH">
        <annotation>
          <documentation>brukes dersom vi ikke vet lysstyrke, men har observert lysfarge hvit fast lys</documentation>
        </annotation>
      </enumeration>
      <enumeration value="BH">
        <annotation>
          <documentation>brukes dersom vi ikke vet lysstyrke, men har observert lysfarge hvitt blinkende lys</documentation>
        </annotation>
      </enumeration>
      <enumeration value="BR">
        <annotation>
          <documentation>brukes dersom vi ikke vet lysstyrke, men har observert lysfarge rødt blinkende lys</documentation>
        </annotation>
      </enumeration>
      <enumeration value="L">
        <annotation>
          <documentation>brukes dersom man vet at hinderet har lys, men ikke vet farge og type</documentation>
        </annotation>
      </enumeration>
      <enumeration value="IL"/>
      <enumeration value="UKJ">
        <annotation>
          <documentation>benyttes dersom det er ukjent om hinderet har lys eller ikke.</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
