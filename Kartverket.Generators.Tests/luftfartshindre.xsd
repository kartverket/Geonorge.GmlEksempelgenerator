      </enumeration>
      <enumeration value="Oljerigg">
        <annotation>
          <documentation>hentet fra AIXM 5.1 klasse CodeVerticalStructureType, kode RIG</documentation>
        </annotation>
      </enumeration>
      <enumeration value="Skilt">
